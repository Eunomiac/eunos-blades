<svg class="svg-defs" viewBox="0 0 400 400">
  <defs>
    {{#each svgData.keys.list as |keyData size|}}
    <path id="{{size}}-key" d="{{keyData.path}}" />
    <clipPath id="{{size}}-key-clip">
      <use href="#{{size}}-key" />
    </clipPath>
    {{/each}}

    <filter id="metallicFilter">
      <feTurbulence
        type="turbulence"
        baseFrequency="0.03"
        numOctaves="3"
        stitchTiles="stitch"
        result="turbulence" />
      <feDisplacementMap
        in2="turbulence"
        in="SourceGraphic"
        scale="660"
        xChannelSelector="B"
        yChannelSelector="B" />
     </filter>
  </defs>
</svg>

{{eLog "CLOCK OVERLAY" this}}

{{#each clockKeys}}
  {{#if (eq ../currentScene sceneID)}}
    {{> "systems/eunos-blades/templates/overlays/clock-key.hbs" this}}
  {{/if}}
{{/each}}